print("Today is a good day to lear Python")
print("Python is fun")
print("Python's string are easy to use")
print("We can even include 'quotes'in strings")
print('hello'+'world')
greeting = "hello"
name = "ayaan"
print(greeting + name)

# If we want a space we can also add a note just like this.
print(greeting + " " + name)


age= 16
print(age)

print(type(greeting))
print(type(age))

age_in_words= "16 years"
print (name + " is " + age_in_words + " years old")
print(type(age))